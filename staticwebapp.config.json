{
    "routes": [
        {
            "route": "/",
            "serve": "/index.html"
        },
        {
            "route": "/*",
            "serve": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/styles.css",
            "/script.js",
            "/*.{js,css,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "application/javascript",
        ".css": "text/css",
        ".html": "text/html",
        ".jpg": "image/jpeg",
        ".jpeg": "image/jpeg",
        ".png": "image/png",
        ".gif": "image/gif",
        ".svg": "image/svg+xml",
        ".ico": "image/x-icon",
        ".woff": "font/woff",
        ".woff2": "font/woff2",
        ".ttf": "font/ttf",
        ".eot": "application/vnd.ms-fontobject"
    },
    "globalHeaders": {
        "Cache-Control": "public, max-age=31536000, immutable"
    },
    "responseOverrides": {
        "404": {
            "redirect": "/",
            "statusCode": 302
        }
    }
}